{
	"version": 1,
	"handlers": {
		"file": {
			"class": "logging.handlers.RotatingFileHandler",
			"formatter": "default",
			"filename": "/var/log/cxoneflow/cxoneflow.log",
			"backupCount" : 10,
			"maxBytes" : 1024000000
		}
	},
	"formatters": {
		"default": {
			"format": "[%(asctime)s][%(process)d][%(name)s][%(levelname)s] %(message)s",
			"datefmt": "%Y-%m-%dT%H:%M:%S%z"
		}
	},
	"loggers": {
		"root": {
			"handlers": ["file"],
			"level": "INFO"
		}
	}
}
